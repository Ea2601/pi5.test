{
  "name": "vite-react-typescript-starter",
  "private": true,
  "version": "2.1.4",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:backend": "node backend/dev-server.js",
    "dev:full": "concurrently \"npm run dev:backend\" \"npm run dev\"",
    "build": "vite build",
    "preview": "vite preview",
    "type-check": "tsc --noEmit",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "clean": "rm -rf node_modules/.cache && rm -rf dist && rm -rf .turbo",
    "analyze": "npm run build && npx vite-bundle-analyzer dist",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "migration:create": "cd supabase && npx supabase db diff --use-migra",
    "migration:apply": "cd supabase && npx supabase db push",
    "schema:generate": "cd supabase && npx supabase gen types typescript --local > ../src/types/database.ts",
    "dev:backend": "cd backend && npm run dev",
    "dev:all": "concurrently \"npm run dev\" \"npm run dev:backend\"",
    "dev:clean": "npm run clean && npm install && npm run dev",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "docker:clean": "docker system prune -a -f && docker volume prune -f",
    "health:check": "curl -f http://localhost:3000/health || exit 1",
    "optimize": "npm run clean && npm ci && npm run lint:fix && npm run type-check && npm run build"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@supabase/supabase-js": "^2.56.1",
    "@tanstack/react-query": "^5.85.5",
    "clsx": "^2.1.1",
    "framer-motion": "^12.23.12",
    "lucide-react": "^0.344.0",
    "qrcode": "^1.5.4",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-helmet-async": "^2.0.5",
    "recharts": "^3.1.2",
    "tailwind-merge": "^3.3.1",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.18",
    "axios": "^1.11.0",
    "concurrently": "^9.2.1",
    "dotenv": "^17.2.1",
    "eslint": "^9.9.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "globals": "^15.9.0",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.3.0",
    "vite": "^5.4.2",
    "winston": "^3.17.0"
  }
}
